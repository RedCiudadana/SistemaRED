<body id="kt_body" class="header-fixed header-mobile-fixed header-bottom-enabled page-loading" style="overflow-x:hidden">

<div class="row" style="margin-bottom:1rem">
    <div class="col-xxl-12">
        <!--begin::Advance Table Widget 3-->
        <div class="card card-custom gutter-b" >
            <!--begin::Header-->
            <div class="card-header border-0 py-5">
                <h3 class="card-title align-items-start flex-column">
                    <span class="card-label font-weight-bolder text-dark">Administración</span>
                </h3>
                <div class="my-lg-0 my-1">
                    <div class="popup-wrapper2">
                        <div class="popup">
                            <div class="popup-close2">x</div>
                            <div class="popup-content">
                                <h2>Copie el código </h2>
                                <code>
                                &lt;embed src="https://sistemaranking.redciudadana.org/administracion/admin2/" style="overflow:hidden" &gt;
                                </code>
                                
                            </div>
                        </div>
                    </div>
                    <button id="boton2" class="btn btn-sm btn-info font-weight-bolder text-uppercase">Compartir<img height="20px" width="20px" style="margin-left:4px" src="/assets/ELEMENTOS/MAPAS_SR-06.png"></button>
                </div>
            </div>
            <!--end::Header-->
            <!--begin::Body-->
            <div class="card-body pt-0 pb-3">
                <!--begin::Table-->
                <div class="table-responsive">
                    <table class="table table-head-custom table-head-bg table-borderless table-vertical-center">
                        <thead>
                            <tr class="text-uppercase">
                                <th style="min-width: 50px">Id</th>
                                <th style="min-width: 150px" class="pl-7">
                                    <span class="text-dark-75">Categoria</span>
                                </th>
                                <th style="min-width: 150px">Documento</th>
                                <th style="min-width: 150px">Prioridad</th>
                                <th style="min-width: 150px">Estado</th>
                                <th style="min-width: 200px">Descarga</th>
                            </tr>
                        </thead>
                        <tbody>
                            {% for item in collections.admin %}
                            <tr>
                                <td>
                                    <span class="text-dark-75 font-weight-bolder d-block font-size-lg">{{item.data.admin.id_administracion}}</span>
                                </td>
                                <td>
                                    <span class="text-dark-75 font-weight-bolder d-block font-size-lg">{{item.data.admin.Categoria}}</span>
                                </td>
                                <td>
                                    <span class="text-dark-75 font-weight-bolder d-block font-size-lg">{{item.data.admin.Documento}}</span>
                                </td>
                                <td>
                                    <span class="text-dark-75 font-weight-bolder d-block font-size-lg">{{item.data.admin.Prioridad}}</span>
                                </td>
                                <td>
                                    <span class="text-dark-75 font-weight-bolder d-block font-size-lg">{{item.data.admin.Estado}}</span>
                                </td>
                                <td class="pl-0 py-8">
                                    <div class="d-flex align-items-center">
                                        <div>
                                            <a href="{{item.data.admin.Enlace}}" class="text-dark-75 font-weight-bolder text-hover-primary mb-1 font-size-lg">{{item.data.admin.TextoDescarga}}</a><br>
                                        </div>
                                    </div>
                                </td>	
                            </tr>
                            {% endfor %}
                            
                        </tbody>
                    </table>
                </div>
                <!--end::Table-->
            </div>
            <!--end::Body-->
        </div>
        <!--end::Advance Table Widget 3-->
    </div>


</div>
