<body id="kt_body" class="header-fixed header-mobile-fixed header-bottom-enabled page-loading" style="overflow-x:hidden">

<div class="row" style="margin-bottom:1rem">

    <div class="col-xl-4" style="margin-bottom:1rem">

        <div class="card-body p-0 d-flex rounded justify-content-center" style="background:radial-gradient(94.09% 94.09% at 50% 50%, rgba(255, 255, 255, 0.45) 0%, rgba(255, 255, 255, 0) 100%), #fff; margin-bottom:1rem;height:100%!important">
            <div class="row m-0">
                <div class=" p-0">
                    <div class="card card-custom card-stretch card-transparent card-shadowless">
                        <div class="card-body d-flex flex-column justify-content-center">
                            {% for item in collections.resadmin %}
                            {% if item.data.resadmin.Prioridad === "Indispensable" %}
                            <h3 class="font-size-h4 font-size-h1-sm font-size-h4-lg font-size-h4-lg font-size-h1-xl mb-0  font-weight-bolder text-center" >
                            Indispensable 
                            </h3>
                            <h3 class="text-center" id="iniciartext" style="font-size:50px;padding-top:5px;font-weight:bold">{{item.data.resadmin.Completo}}/{{item.data.resadmin.Sumatotal}}</h3>
                            {% endif %}
                            <br>
                            {% if item.data.resadmin.Prioridad === "Importante" %}
                            <h3 class="font-size-h4 font-size-h1-sm font-size-h4-lg font-size-h4-lg font-size-h1-xl mb-0  font-weight-bolder text-center" >
                            Importante
                            </h3>
                            <h3 class="text-center" id="procesotext" style="font-size:50px;padding-top:5px;font-weight:bold">{{item.data.resadmin.Completo}}/{{item.data.resadmin.Sumatotal}}</h3>
                            {% endif %}
                            <br>
                            {% if item.data.resadmin.Prioridad === "Relevante" %}
                            <h3 class="font-size-h4 font-size-h1-sm font-size-h4-lg font-size-h4-lg font-size-h1-xl mb-0  font-weight-bolder text-center" >
                            Relevante
                            </h3>
                            <h3 class="text-center" id="finalizadotext" style="font-size:50px;padding-top:5px;font-weight:bold">{{item.data.resadmin.Completo}}/{{item.data.resadmin.Sumatotal}}</h3>
                            {% endif %}
                            {% endfor %}
                        </div>
                    </div>
                </div>
                
            </div>
        </div>

    </div>

    <div class="col-xl-4" style="margin-bottom:1rem">

        <div class="card-body p-0 d-flex rounded justify-content-center" style="background:radial-gradient(94.09% 94.09% at 50% 50%, rgba(255, 255, 255, 0.45) 0%, rgba(255, 255, 255, 0) 100%), #fff; margin-bottom:1rem;height:100%!important">
            <div class="row m-0">
                <div class=" p-0">
                    <div class="card card-custom card-stretch card-transparent card-shadowless">
                        <div class="card-body d-flex flex-column justify-content-center">
                            {% for item in collections.resadmin %}
                            {% if item.data.resadmin.Prioridad === "Indispensable" %}
                            <h3 class="font-size-h4 font-size-h1-sm font-size-h4-lg font-size-h4-lg font-size-h1-xl mb-0  font-weight-bolder text-center" >
                            Completo
                            </h3>
                            <h3 class="text-center" id="iniciartext" style="font-size:50px;padding-top:5px;font-weight:bold">{{item.data.resadmin.Completo}}</h3>
                            
                            <br>

                            <h3 class="font-size-h4 font-size-h1-sm font-size-h4-lg font-size-h4-lg font-size-h1-xl mb-0  font-weight-bolder text-center" >
                            En proceso
                            </h3>
                            <h3 class="text-center" id="procesotext" style="font-size:50px;padding-top:5px;font-weight:bold">{{item.data.resadmin.Enproceso}}</h3>
                            
                            <br>

                            <h3 class="font-size-h4 font-size-h1-sm font-size-h4-lg font-size-h4-lg font-size-h1-xl mb-0  font-weight-bolder text-center" >
                            Pendiente
                            </h3>
                            <h3 class="text-center" id="finalizadotext" style="font-size:50px;padding-top:5px;font-weight:bold">{{item.data.resadmin.Pendiente}}</h3>
                            {% endif %}
                            {% endfor %}
                        </div>
                    </div>
                </div>
                
            </div>
        </div>

    </div>

    
        
    <div class="col-xl-4" style="margin-bottom:1rem" >
        
        <div class="card-body p-0 d-flex rounded justify-content-center" style="background:radial-gradient(94.09% 94.09% at 50% 50%, rgba(255, 255, 255, 0.45) 0%, rgba(255, 255, 255, 0) 100%), #fff; margin-bottom:1rem;height:100%!important">
            <div class="row m-0">
                <div class=" p-0">
                    <div class="card card-custom card-stretch card-transparent card-shadowless">
                        <div class="card-body d-flex flex-column justify-content-center">
                            <h3 class="font-size-h4 font-size-h1-sm font-size-h4-lg font-size-h4-lg font-size-h1-xl mb-0  font-weight-bolder text-center" >
                                Avance
                            </h3>

                            {% for item in collections.resadmin %}
                            {% if item.data.resadmin.Prioridad === "Indispensable" %}
                            <p id="completo" style="display:none">{{item.data.resadmin.PorcentajeCompleto}} </p>
                            <p id="proceso" style="display:none">{{item.data.resadmin.PorcentajeProceso}} </p>
                            <p id="pendiente" style="display:none">{{item.data.resadmin.PorcentajePendiente}} </p>
                            {% endif %}
                            {% endfor %}

                            <div id="chart_11"></div>
                        </div>
                    </div>
                </div>	
            </div>
        </div>
    </div>


