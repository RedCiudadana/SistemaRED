													</div>
													<div class="d-flex align-items-center">
														<span class="font-weight-bold mr-4">Fecha Final: </span>
														<span class="btn btn-light-danger btn-sm font-weight-bold btn-upper btn-text">{{item.data.proyecto.FechaCierre}}</span>
													</div>
												</div>
												<!--end::Data-->
												<!--begin::Progress-->
