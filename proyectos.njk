														<div class="d-flex flex-column mr-auto">
															<a href="{{item.url}}" class="text-dark text-hover-primary font-size-h4 font-weight-bolder mb-1"> {{item.data.proyecto.Proyecto}}</a>
														</div>
														<!--end::Title-->
													</div>
													<!--end::Info-->
													
												</div>
												<!--end::Info-->
												<!--begin::Description-->
